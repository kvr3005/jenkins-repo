pipeline{
	agent any 
	stages {
		stage('git-checkout') {
			steps {
				echo "Checking out from Git Repo"
			}
		}
		stage('Build') {
			steps {
				echo "Build Successfully"
			}
		}
		stage('Test') {
			steps {
				echo "Test passed Successfully"
			}
		}
		stage('Deploy') {
			steps {
					echo "Deploy successfully"
			}
		}
		
	}
}
